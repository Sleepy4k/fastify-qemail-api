{
  "name": "fastify-qemail-api",
  "version": "1.0.0",
  "description": "High-performance disposable email service API built with Bun and Fastify",
  "main": "src/server.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch src/server.ts",
    "start": "bun src/server.ts",
    "build": "bun build src/server.ts --compile --outfile qemail-api",
    "db:migrate": "bun run database/migrate.ts",
    "db:seed": "bun run database/seed.ts",
    "test": "bun test",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "fastify",
    "bun",
    "temporary-email",
    "disposable-email",
    "cloudflare",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@fastify/cookie": "^10.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "@fastify/swagger": "^9.3.0",
    "@fastify/swagger-ui": "^5.0.1",
    "@sinclair/typebox": "^0.33.15",
    "argon2": "^0.41.1",
    "fastify": "^5.2.0",
    "fastify-plugin": "^5.0.1",
    "mysql2": "^3.11.5",
    "nanoid": "^5.0.9",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.10.2",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=20.0.0"
  }
}
